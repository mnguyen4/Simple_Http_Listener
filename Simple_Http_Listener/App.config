<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
        <!-- Enable the use of the hosts file to allow custom host names -->
        <add key="useHostsFile" value="false"/>
        <!-- Custom host names can be delimited by a ';' -->
        <add key="hostName" value="127.0.0.1;localhost"/>
    </appSettings>
</configuration>